     "text": [
      "hello from func\n",
      "bye from conman\n"
     ]
    }
   ],
   "source": [
    "from contextlib import contextmanager\n",
    "\n",
    "@contextmanager\n",
    "def myconman():\n",
    "    a = \"hello\" \n",
    "    yield a\n",
    "    a = \"bye\"\n",
    "    print(f\"{a} from conman\")\n",
    "\n",
    "\n",
    "with myconman() as cm:\n",
    "    print(f\"{cm} from func\")\n",
    "\n"
   ]
  }
 ],
